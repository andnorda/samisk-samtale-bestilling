{
    "name": "samisk-samtale-bestilling",
    "version": "1.2.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "npm-run-all build:*",
        "build:client": "vite build --outDir ./server/dist/client",
        "build:ssr": "vite build --outDir ./server/src/_ssr-dist --ssr src/main-server.tsx",
        "build:server": "npm run build --workspace server",
        "analyze-bundle": "ANALYZE=true npm run build:client",
        "start": "npm run start --workspace server",
        "lint": "eslint",
        "start-local": "cp .env.local-prod .env && npm run start --workspace server",
        "decorator-local": "docker compose up",
        "dev-prepare": "node preact-compat-symlinks.js && cp .env.development .env",
        "dev": "npm run dev-prepare && npm run dev --workspace server"
    },
    "workspaces": [
        "server"
    ],
    "dependencies": {
        "@navikt/ds-css": "4.4.1",
        "@navikt/ds-react": "4.4.1",
        "@navikt/nav-dekoratoren-moduler": "2.1.1",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "amplitude-js": "8.21.9",
        "classnames": "2.3.2",
        "html-react-parser": "4.0.0",
        "jsdom": "22.1.0",
        "eslint": "8.42.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "node-cache": "5.1.2",
        "@preact/compat": "17.1.2",
        "preact-render-to-string": "5.2.6",
        "react-router-dom": "6.8.1"
    },
    "devDependencies": {
        "@babel/preset-react": "7.18.6",
        "@preact/preset-vite": "2.5.0",
        "@types/node": "18.16.18",
        "@types/react": "18.2.7",
        "@types/react-dom": "18.0.11",
        "eslint": "8.41.0",
        "eslint-config-next": "13.4.4",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.8",
        "typescript": "5.1.3",
        "rollup-plugin-visualizer": "5.9.0",
        "husky": "8.0.0",
        "vite": "4.4.3"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
